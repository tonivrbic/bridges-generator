{
  "name": "bridges-generator",
  "version": "0.0.2",
  "description": "Generates a Hashi puzzle",
  "main": "lib/index.cjs.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs.js",
      "import": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "format": "prettier --write \"./{src,test}/**/*.{ts,js}\"",
    "prepare": "npm run format && npm run test && npm run build",
    "test": "tsc && node ./out/test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonivrbic/bridges-generator.git"
  },
  "keywords": [
    "hashi",
    "hashiwokakero",
    "bridges",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tonivrbic/bridges-generator/issues"
  },
  "homepage": "https://github.com/tonivrbic/bridges-generator#readme",
  "dependencies": {
    "bridges-solver": "^1.1.0"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "husky": "^3.0.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rollup": "~1.20.1",
    "rollup-plugin-delete": "~1.1.0",
    "rollup-plugin-dts": "~1.1.6",
    "rollup-plugin-typescript2": "~0.23.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.3"
  }
}
